class Conver{
	private static double rate;
	
	public static double Doll(double won) {
		return won / rate;
	}
	
	public static double KWR(double dollar) {
		return dollar * rate;
	}
	
	public static double Rate(double r) {
		rate = r;
	}
}

public class Ex_20230526 {
	public static void main(String[] args) {
		
	}
}
	}